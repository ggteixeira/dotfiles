! -*- coding: utf-8 -*-

! Swap left-alt with alt Gr (Level 3 Latch)
remove mod1 = Meta_L
remove mod5 = Mode_switch
keycode 64 = 
keycode 64 = ISO_Level3_Latch NoSymbol ISO_Level3_Latch

! Swaps Right Super w/ Left Alt 
clear mod4
add mod4 = Super_L
add mod3 = Super_R
keycode 134 = NoSymbol
keycode 134 = Alt_L

! Swaps Esc with Caps Lock
remove lock = Caps_Lock
keycode 66 = 
keycode 66 = Escape

keycode 9 = 
