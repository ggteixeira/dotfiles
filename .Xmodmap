! -*- coding: utf-8 -*-

! Swap left-alt with alt Gr (Level 3 Latch)
remove mod1 = Meta_L
remove mod5 = Mode_switch
keycode 64 = 
keycode 64 = ISO_Level3_Latch NoSymbol ISO_Level3_Latch

! Swap Right Super w/ Left Alt 
clear mod4
add mod4 = Super_L
add mod3 = Super_R
keycode 134 = NoSymbol
keycode 134 = Alt_R

! Swap Esc with Caps Lock
clear lock
keycode 66 = 
keycode 9 = 
keycode 66 = Escape
keycode 9 = 
add lock = Caps_Lock
